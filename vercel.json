{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "files/**",
          "client/build/**"
        ]
      }
    }
  ],
  "routes": [
    {        
      "src": "/(.*)",        
      "dest": "server.js"
    }
  ]
}